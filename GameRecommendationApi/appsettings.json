{
  "Neo4j": {
    "Uri": "${NEO4J_URI}",
    "Username": "${NEO4J_USERNAME}",
    "Password": "${NEO4J_PASSWORD}"
  },
  "Authorization": {
    "ApiKey": "${AUTHORIZATION_APIKEY}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
